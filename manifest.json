{
  "name": "FinGuid USA Mortgage Calculator - World's First AI-Enhanced Calculator",
  "short_name": "FinGuid USA Mortgage",
  "description": "America's most advanced mortgage calculator with AI insights, live rates, voice control, and comprehensive loan analysis for USA homebuyers. FHA, VA, USDA, and conventional loans supported.",
  "version": "9.0.0",
  "start_url": "/mortgage-calculator?source=pwa",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#1E3A8A",
  "background_color": "#ffffff",
  "lang": "en-US",
  "dir": "ltr",
  "scope": "/",
  "categories": ["finance", "business", "productivity", "utilities"],
