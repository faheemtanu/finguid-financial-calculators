  "shortcuts": [
    {
      "name": "Quick Calculate",
      "short_name": "Calculate",
      "description": "Quickly calculate mortgage payments",
      "url": "/mortgage-calculator?action=calculate",
      "icons": [
        {
          "src": "/assets/shortcuts/calculate-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Current Rates", 
      "short_name": "Rates",
      "description": "View live USA mortgage rates",
      "url": "/rates",
      "icons": [
        {
          "src": "/assets/shortcuts/rates-96x96.png", 
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Saved Calculations",
      "short_name": "Saved",
      "description": "View saved mortgage calculations",
      "url": "/saved-calculations",
      "icons": [
        {
          "src": "/assets/shortcuts/saved-96x96.png",
          "sizes": "96x96" 
        }
      ]
    },
    {
      "name": "Loan Comparison",
      "short_name": "Compare", 
      "description": "Compare different loan scenarios",
      "url": "/mortgage-calculator?tab=compare",
      "icons": [
        {
          "src": "/assets/shortcuts/compare-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share-calculation",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+mortgage",
      "url": "/mortgage-calculator?calculation=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.finguid.mortgagecalculator",
      "id": "com.finguid.mortgagecalculator"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/us/app/finguid-mortgage-calculator/id123456789"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "handle_links": "preferred",
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone", "browser"],
  "file_handlers": [
    {
      "action": "/import-calculation",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"]
      },
      "icons": [
        {
          "src": "/assets/icons/file-import-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ]
}
